{
  "name": "@nhealth/nutils",
  "version": "0.0.2",
  "description": "Shared Nuxt.js v4 module with components, composables, and utilities",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "style": "./dist/runtime/app/assets/styles.css"
    }
  },
  "main": "./dist/module.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "build": "yarn dev:prepare && nuxt-module-build build",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bumpp --recursive && npm publish",
    "bumpp": "bumpp --recursive",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.23.0",
    "@nuxt/ui": "^4.3.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.2",
    "bumpp": "10.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhealthorg/nutils.git"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt4",
    "components",
    "composables",
    "utilities"
  ],
  "author": "nhealth.org",
  "license": "MIT"
}
